{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "indent": [true, "tabs", 4],
        "semicolon": [true, "never"],
        "object-literal-sort-keys": false,
        "no-string-literal": false,
        "trailing-comma": [false, {"multiline": "never", "singleline": "never"}]
    },
    "rulesDirectory": []
}
